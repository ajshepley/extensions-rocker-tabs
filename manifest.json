{
    "manifest_version": 2,
    "name": "Rocker-Tabs",
    "version": "1.0",

    "description": "Adds middle-click based Rocker gestures for prevtab and nexttab.",

    "icons": {
        "48": "icons/border-48.png"
    },

    "content_scripts": [
        {
            "matches": ["*://*.mozilla.org/*"],
            "js": ["rocker-tab.js"]
        }
    ]
}